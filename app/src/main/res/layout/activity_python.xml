<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Python">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="34dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <ScrollView
        android:layout_width="409dp"
        android:layout_height="729dp"
        app:layout_constraintTop_toTopOf="@+id/guideline8"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
<!--Quesion 1 -->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="1. Which of the following is NOT a feature of Python?"
                android:textSize="19dp"
                >
            </TextView>

           <CheckBox
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               android:text="Python is an interpreted language."
               android:textSize="16dp">
           </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Python supports both object-oriented and functional programming"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Python uses static typing"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Python has a large standard library."
                android:textSize="16dp"
                >
            </CheckBox>

            <Space
                android:layout_width="20dp"
                android:layout_height="20dp">

            </Space>
<!--Quesion 2-->

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="2. In Python, which of the following data types is immutable?"
                android:textSize="19dp"></TextView>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="List"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=" Dictionary"
                android:textSize="16dp">
            </CheckBox>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=" String"
                android:textSize="16dp"></CheckBox>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Set"
                android:textSize="16dp">
            </CheckBox>

            <Space
                android:layout_width="20dp"
                android:layout_height="20dp">

            </Space>

            <!--Quesion 3-->

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="3. What is the purpose of the self keyword in a Python class method?"
                android:textSize="19dp"></TextView>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="To refer to the class itself"
                android:textSize="16dp"></CheckBox>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="  To access class variables"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="To access instance attributes and methods"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text=" To declare a class as static"
                android:textSize="16dp">
            </CheckBox>

            <Space
                android:layout_width="20dp"
                android:layout_height="20dp">

            </Space>

            <!--Quesion 4-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="4. Which of the following statements about Python functions is TRUE?"
                android:textSize="19dp"
                >
            </TextView>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="A) A function can only return one value."
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="B) Functions cannot accept other functions as arguments."
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="C) Functions are first-class objects in Python."
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="D) Functions cannot have default argument values."
                android:textSize="16dp">
            </CheckBox>

            <Space
                android:layout_width="20dp"
                android:layout_height="20dp">

            </Space>

            <!--Quesion 5-->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="5. What is the main purpose of the Python __init__ method?"
                android:textSize="19dp"
                >
            </TextView>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="A) To initialize a class before it is created"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="B) To define a class-level attribute"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="C) To act as a constructor and initialize the object's state"
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="D) To define private variables"
                android:textSize="16dp">
            </CheckBox>

            <Space
                android:layout_width="20dp"
                android:layout_height="20dp">

            </Space>

            <!--Quesion 6 -->
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="6. Which of the following statements about Python modules is FALSE?"
                android:textSize="19dp"
                >
            </TextView>

            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="A) Modules allow code to be organized into reusable components."
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="B) A Python file with a .py extension can be imported as a module."
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="C) Once a module is imported, it cannot be re-imported."
                android:textSize="16dp">
            </CheckBox>
            <CheckBox
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="D) The import statement is used to load a module."
                android:textSize="16dp">
            </CheckBox>

            <Space
                android:layout_width="20dp"
                android:layout_height="20dp">

            </Space>

         <!--Quesion 7-->
         <TextView
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="7. In Python, which of the following statements about the global keyword is correct?"
             android:textSize="19dp"
             >
         </TextView>

         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="A) It allows modification of a local variable."
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="B) It allows modification of a global variable from within a function."
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="C) It creates a new global variable within a function."
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="D) It deletes a global variable."
             android:textSize="16dp">
         </CheckBox>

         <Space
             android:layout_width="20dp"
             android:layout_height="20dp">

         </Space>

         <!--Quesion 8-->
         <TextView
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="8. Which of the following is NOT a valid way to create a set in Python?
A) my_set = set()"
             android:textSize="19dp"
             >
         </TextView>

         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="A) my_set = set()"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="B) my_set = {1, 2, 3}"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="C) my_set = {}"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="D) my_set = set([1, 2, 3])"
             android:textSize="16dp">
         </CheckBox>

         <Space
             android:layout_width="20dp"
             android:layout_height="20dp">

         </Space>


         <!--Quesion 9-->
         <TextView
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="9. Which of the following keywords is used for exception handling in Python?"
             android:textSize="19dp"
             >
         </TextView>

         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="A) error"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="B) throw"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="C) except"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="D) raise"
             android:textSize="16dp">
         </CheckBox>

         <Space
             android:layout_width="20dp"
             android:layout_height="20dp">

         </Space>



         <!--Quesion 10-->
         <TextView
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="10. What does the Python pass statement do?"
             android:textSize="19dp"
             >
         </TextView>

         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="A) Terminates the program"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="B) Does nothing and allows the program to continue"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="C) Skips an error"
             android:textSize="16dp">
         </CheckBox>
         <CheckBox
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:text="D) Stops a loop"
             android:textSize="16dp">
         </CheckBox>

         <Space
             android:layout_width="20dp"
             android:layout_height="20dp">
         </Space>



        </LinearLayout>





    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>